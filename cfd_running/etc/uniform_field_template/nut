FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 8.58e-06; // ~ nu 8.58e-06 for SA

boundaryField
{
    FARFIELD
    {
        type            freestream;
        freestreamValue   $internalField;
    }
    FRONT
    {
        type            empty;
    }
    BACK
    {
        type            empty;
    }
    wall
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

}

