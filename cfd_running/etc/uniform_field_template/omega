     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

// omega = k^0.5/l or k^0.5/(cmu^0.25*l)

internalField   uniform 100;

boundaryField
{
  FARFIELD
  {
    type freestream;
    freestreamValue $internalField;
  }
  FRONT
  {
    type empty;
  }
  BACK
  {
    type empty;
  }
  wall
  {
    type            omegaWallFunction;
    blending        binomial2;
    value           $internalField;
}

}


// ************************************************************************* //
