FoamFile
{
version     2.0;
format      ascii;
class       volVectorField;
location    "0";
object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0); //AoA 0deg

boundaryField
{
FARFIELD
{
type            freestreamVelocity;
freestreamValue	$internalField;
}
FRONT
{
type            empty;
}
BACK
{
type            empty;
}
wall
{
type            fixedValue;
value           uniform (0 0 0);
//type            noSlip;
}
}

