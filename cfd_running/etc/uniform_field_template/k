FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

//k = 3/2 * (I *U)^2 

internalField   uniform 0.375;

boundaryField
{
    FARFIELD
    {
        type            	freestream;
        freestreamValue	$internalField;
    }
    FRONT
    {
        type            empty;
    }
    BACK
    {
        type            empty;
    }
    wall
   {
        type            kqRWallFunction;
        value           $internalField;
    }

}


