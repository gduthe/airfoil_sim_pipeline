FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

//epsilon = cmu*k^1.5/L or cmu^0.75

internalField   uniform 14.855;

boundaryField
{
  FARFIELD
  {
    type freestream;
    freestreamValue $internalField;
  }
  FRONT
  {
    type empty;
  }
  BACK
  {
    type empty;
  }
  wall
  {
    type            epsilonWallFunction;
    value           $internalField;
   }
}


// ************************************************************************* //
