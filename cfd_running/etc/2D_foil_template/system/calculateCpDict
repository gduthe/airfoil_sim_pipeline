FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      calculateCpCfDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

	// name of patch of interest
	bodyPatch	AIRFOIL;

	// name of pressure field: p or pMean
	definedPressureField	p;

	// exact position of on span (for 2D meshes only)
	span		0.0;
	spanAxis	(0 0 1);


